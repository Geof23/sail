# PNG codec information
#
[codec]
layout=3
version=1.1.3
name=PNG
description=Portable Network Graphics
magic-numbers=89 50 4E 47 0D 0A 1A 0A
extensions=png@CODEC_INFO_EXTENSION_APNG@
mime-types=image/png

[read-features]
features=STATIC@CODEC_INFO_FEATURE_ANIMATED@;META-INFO;INTERLACED;ICCP
output-pixel-formats=SOURCE;BPP24-RGB;BPP24-BGR;BPP32-RGBA;BPP32-BGRA;BPP32-ARGB;BPP32-ABGR
default-output-pixel-format=@SAIL_DEFAULT_READ_OUTPUT_PIXEL_FORMAT@

[write-features]
features=STATIC;META-INFO;INTERLACED;ICCP
properties=
interlaced-passes=7
compression-types=DEFLATE
default-compression=DEFLATE
compression-level-min=1
compression-level-max=9
compression-level-default=6
compression-level-step=1

[write-pixel-formats-mapping]
BPP1-INDEXED=SOURCE
BPP2-INDEXED=SOURCE
BPP4-INDEXED=SOURCE
BPP8-INDEXED=SOURCE
BPP24-RGB=SOURCE
BPP24-BGR=SOURCE
BPP48-RGB=SOURCE
BPP48-BGR=SOURCE
BPP32-RGBA=SOURCE
BPP32-BGRA=SOURCE
BPP32-ARGB=SOURCE
BPP32-ABGR=SOURCE
BPP64-RGBA=SOURCE
BPP64-BGRA=SOURCE
BPP64-ARGB=SOURCE
BPP64-ABGR=SOURCE
