# JPEG plugin information
#
[plugin]
layout=2
version=1.3.4.1
name=JPEG
description=Joint Photographic Experts Group
extensions=jpg;jpeg;jpe;jif;jfif;jfi
mime-types=image/jpeg

[read-features]
input-pixel-formats=BPP8-GRAYSCALE;BPP24-RGB;BPP24-YCBCR;BPP32-CMYK;BPP32-YCCK
output-pixel-formats=SOURCE;BPP8-GRAYSCALE;BPP16-RGB565;BPP24-RGB;BPP24-BGR;BPP32-RGBX;BPP32-BGRX;BPP32-XBGR;BPP32-XRGB;BPP32-RGBA;BPP32-BGRA;BPP32-ABGR;BPP32-ARGB;BPP24-YCBCR;BPP32-CMYK;BPP32-YCCK
preferred-output-pixel-format=BPP24-RGB
features=STATIC;META-INFO

[write-features]
input-pixel-formats=BPP8-GRAYSCALE;BPP16-RGB565;BPP24-RGB;BPP24-BGR;BPP32-RGBX;BPP32-BGRX;BPP32-XBGR;BPP32-XRGB;BPP32-RGBA;BPP32-BGRA;BPP32-ABGR;BPP32-ARGB;BPP24-YCBCR;BPP32-CMYK;BPP32-YCCK
output-pixel-formats=SOURCE;BPP8-GRAYSCALE;BPP24-RGB;BPP24-YCBCR;BPP32-CMYK;BPP32-YCCK
preferred-output-pixel-format=BPP24-YCBCR
features=STATIC;META-INFO
properties=
passes=0
compression-types=
preferred-compression-type=
compression-min=0
compression-max=100
compression-default=15
