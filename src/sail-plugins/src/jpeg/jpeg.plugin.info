# JPEG plugin information
#
[plugin]
layout=2
version=1.3.4.1
name=JPEG
description=Joint Photographic Experts Group
extensions=jpg;jpeg;jpe;jif;jfif;jfi
mime-types=image/jpeg

[read-features]
features=STATIC;META-INFO
output-pixel-formats=SOURCE;BPP24-RGB;BPP32-RGBA
preferred-output-pixel-format=BPP24-RGB

#[read-pixel-formats-mapping]
#BPP8-GRAYSCALE=SOURCE;BPP16-RGB565;BPP24-RGB;BPP24-BGR;BPP32-RGBA;BPP32-BGRA;BPP32-ABGR;BPP32-ARGB
#BPP24-RGB=SOURCE;BPP8-GRAYSCALE;BPP16-RGB565;BPP24-BGR;BPP32-RGBA;BPP32-BGRA;BPP32-ABGR;BPP32-ARGB;BPP24-YCBCR
#BPP24-YCBCR=SOURCE;BPP8-GRAYSCALE;BPP16-RGB565;BPP24-RGB;BPP24-BGR;BPP32-RGBA;BPP32-BGRA;BPP32-ABGR;BPP32-ARGB
#BPP32-CMYK=SOURCE;BPP32-YCCK
#BPP32-YCCK=SOURCE;BPP32-CMYK

[write-features]
#preferred-output-pixel-formats=BPP24-YCBCR;SOURCE
features=STATIC;META-INFO
properties=
passes=0
compression-types=
preferred-compression-type=
compression-min=0
compression-max=100
compression-default=15

[write-pixel-formats-mapping]
BPP8-GRAYSCALE=SOURCE
BPP24-RGB=SOURCE;BPP24-YCBCR;BPP8-GRAYSCALE
BPP24-BGR=BPP24-YCBCR;BPP24-RGB;BPP8-GRAYSCALE
BPP32-RGBA=BPP24-YCBCR;BPP24-RGB;BPP8-GRAYSCALE
BPP32-BGRA=BPP24-YCBCR;BPP24-RGB;BPP8-GRAYSCALE
BPP32-ABGR=BPP24-YCBCR;BPP24-RGB;BPP8-GRAYSCALE
BPP32-ARGB=BPP24-YCBCR;BPP24-RGB;BPP8-GRAYSCALE
BPP24-YCBCR=SOURCE;BPP24-RGB;BPP8-GRAYSCALE
BPP32-CMYK=SOURCE;BPP32-YCCK
BPP32-YCCK=SOURCE;BPP32-CMYK
