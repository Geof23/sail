# PNG plugin information
#
[plugin]
layout=2
version=1.1.3
name=PNG
description=Portable Network Graphics
extensions=png
mime-types=image/png

[read-features]
input-pixel-formats=BPP1-GRAYSCALE;BPP2-GRAYSCALE;BPP4-GRAYSCALE;BPP8-GRAYSCALE;BPP16-GRAYSCALE;BPP8-GRAYSCALE-ALPHA;BPP16-GRAYSCALE-ALPHA;BPP1-INDEXED;BPP2-INDEXED;BPP4-INDEXED;BPP8-INDEXED;BPP24-RGB;BPP48-RGB;BPP32-RGBA;BPP64-RGBA
output-pixel-formats=SOURCE;BPP24-RGB;BPP24-BGR;BPP32-RGBA;BPP32-BGRA;BPP32-ARGB;BPP32-ABGR
preferred-output-pixel-format=BPP32-RGBA
features=STATIC;META-INFO;INTERLACED

[write-features]
input-pixel-formats=BPP24-RGB;BPP24-BGR;BPP48-RGB;BPP48-BGR;BPP32-RGBA;BPP32-BGRA;BPP32-ARGB;BPP32-ABGR;BPP64-RGBA;BPP64-BGRA;BPP64-ARGB;BPP64-ABGR
output-pixel-formats=SOURCE
preferred-output-pixel-format=SOURCE
features=STATIC;META-INFO;INTERLACED
properties=
interlaced-passes=7
compression-types=
preferred-compression-type=
compression-min=1
compression-max=9
compression-default=6
